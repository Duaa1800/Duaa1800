public class CustomerService {
    public static void main(String[] args) {
        String[] customers = {"John", "jane", "Alice", "john", "JANE", "Bob", "alice"};
        boolean[] checked = new boolean[customers.length];

        System.out.println("Duplicate customer names:");
        for (int i = 0; i < customers.length; i++) {
            if (checked[i]) continue; // Skip already checked names
            for (int j = i + 1; j < customers.length; j++) {
                if (customers[i].equalsIgnoreCase(customers[j])) {
                    System.out.println(customers[i]);
                    checked[j] = true; // Mark as checked
                }
            }
        }
    }
}
