class PaymentProcessor {
    // Default payment method
    public void processPayment() {
        System.out.println("Processing default payment.");
    }

    
    public void processPayment(int amount) {
        System.out.printf("Processing payment of amount %d.%n", amount);
    }

   
    public void processPayment(int amount, String customerName) {
        System.out.printf("Processing payment of %d for %s.%n", amount, customerName);
    }
}

public class Main {
    public static void main(String[] args) {
        PaymentProcessor processor = new PaymentProcessor();
        
        processor.processPayment(); // No parameters
        processor.processPayment(100); // One integer parameter
        processor.processPayment(200, "Alice"); // Two parameters
    }
}

