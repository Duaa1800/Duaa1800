class Box {
    private double w, h, d;

    public Box(double w, double h, double d) {
        this.w = w; this.h = h; this.d = d;
    }

    public double volume() {
        return w * h * d;
    }
}

class BoxWeight extends Box {
    private double weight;

    public BoxWeight(double w, double h, double d, double weight) {
        super(w, h, d);
        this.weight = weight;
    }

    public double getWeight() {
        return weight;
    }
}

class Shipment extends BoxWeight {
    private double cost;

    public Shipment(double w, double h, double d, double weight, double cost) {
        super(w, h, d, weight);
        this.cost = cost;
    }

    public double getCost() {
        return cost;
    }
}

public class Main {
    public static void main(String[] args) {
        Shipment s = new Shipment(2, 3, 4, 5, 10);
        System.out.printf("Volume: %.2f, Weight: %.2f, Cost: %.2f%n", s.volume(), s.getWeight(), s.getCost());
    }
}

